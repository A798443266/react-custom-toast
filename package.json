{
  "name": "react-custom-toast",
  "version": "0.1.3",
  "main": "lib/index.umd.js",
  "types": "lib/index.d.ts",
  "author": "zhzxang",
  "license": "MIT",
  "description": "基于 react 的 toast 组件。可以完全的自定义样式。并提供了内置动画与控制方法",
  "keywords": [
    "react",
    "notification",
    "toast",
    "react-component",
    "react-custom-toast",
    "push",
    "message",
    "alert"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:zhzxang/react-custom-toast.git"
  },
  "scripts": {
    "dev": "webpack-dev-server --mode=development --config build/dev.js --port 3010 --open",
    "build:exp": "webpack --mode=production --config build/dev.js",
    "build": "npm run clean && npm run build:lib && npm run build:types",
    "build:lib": "webpack --mode=production --config build/package.umd.js",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rm -rf ./lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-classes": "^7.2.2",
    "@babel/plugin-transform-react-jsx": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-transition-group": "^2.0.15",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "highlight.js": "^9.13.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "react-markdown": "^4.0.6",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-transition-group": "^2.5.2"
  }
